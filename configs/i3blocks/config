[kernel]
command=uname -n -r
interval=once

[volume-pipewire]
command=~/i3blocks-contrib-master/volume-pipewire/volume-pipewire
interval=persist
SUBSCRIBE=1
USE_ALSA_NAME=1
LONG_FORMAT="${VOL}% [${INDEX}:${NAME}]"

[date]
command=date +%d/%m/%y
interval=once

#Update time every 5 seconds
[time]
command=date +%T
interval=1

[battery]
command= upower -i /org/freedesktop/UPower/devices/battery_BAT1 | grep percentage | cut -c 26,27,28,29,30
interval=30
